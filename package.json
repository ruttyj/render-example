    "url": "https://github.com/ruttyj/ReactBase/issues"
  },
  "dependencies": {
    "concurrently": "latest",
    "dotenv": "^8.2.0"
  },
  "Home": "https://github.com/ruttyj/ReactBase#readme"
}
